/**
 * Test function for running a "pure" KAT from [PQC-KAT].
 *
 * "pure" KATs test the top-level `KeyGen`, `Sign`, `Verify` functions.
 *
 * The [PQC-KAT] vectors provide the expected signature in the format
 * `sig || msg`, while [FIPS-204] has sign and verify interact with just `sig`.
 */
tryPureKAT xi expected_pk expected_sk msg expected_sigmsg ctx rnd = mldsaWorks where
    keys = ML_DSA_44::KeyGen (Some xi)

    keygenWorks = case keys of
        None -> False
        Some ks -> (pk == expected_pk) && (sk == expected_sk)
            where (pk, sk) = ks

    sig = ML_DSA_44::Sign expected_sk msg ctx (Some (rndValue rnd))
    signWorks = case sig of
        None -> False
        Some s -> expected_sigmsg == (s # (split msg))

    verifyWorks = case ML_DSA_44::Verify expected_pk msg (take expected_sigmsg) ctx of
        None -> False
        Some b -> b

    mldsaWorks = keygenWorks /\ signWorks /\ verifyWorks

