/**
 * Known answer tests for ML_DSA_44.
 *
 * These can be drawn from several trusted sources, including
 * [PQC-KAT]: Kris Kwiatkowski and Roderick Chapman. Post-quantum-cryptography.
 *     GitHub repo "KAT".
 *     @see https://github.com/post-quantum-cryptography/KAT
 * [ACVP]: NIST. usnistgov. GitHub repo "ACVP-Server".
 *     @see https://github.com/usnistgov/ACVP-Server/tree/master/gen-val/json-files/ML-DSA-keyGen-FIPS204
 *
 * @copyright Galois, Inc
 * @author Marcella Hastings <marcella@galois.com>
 */
module Primitive::Asymmetric::Signature::ML_DSA::Tests::ML_DSA_44 where
import Primitive::Asymmetric::Signature::ML_DSA::Instantiations::ML_DSA_44 as ML_DSA_44
import submodule ML_DSA_44::TestAPI as ML_DSA_Raw

/**
 * The `rnd` parameter for signing provides randomness to prevent side-channel
 * and fault attacks against deterministic signatures.
 *
 * [FIPS-204] allows two types of signatures:
 * - Deterministic signatures set `rnd` to zero.
 * - Hedged signatures use 32 bytes of suitably-generated randomness.
 */
enum Rnd = Deterministic | Hedged ([32][8])

rndValue : Rnd -> [32][8]
rndValue rnd = case rnd of
    Deterministic -> zero
    Hedged v -> v

/**
 * Test function for running a "raw" KAT from [PQC-KAT].
 *
 * "raw" KATs test the `_internal` suite of functions.
 *
 * The [PQC-KAT] vectors provide the expected signature in the format
 * `sig || msg`, while [FIPS-204] has sign and verify interact with just `sig`.
 */
tryRawKAT xi expected_pk expected_sk msg expected_sig rnd = mldsaWorks where
    (pk, sk) = ML_DSA_Raw::KeyGen_internal xi
    keygenWorks = (pk == expected_pk) && (sk == expected_sk)

    sig = ML_DSA_Raw::Sign_internal expected_sk msg (rndValue rnd)
    signWorks = expected_sig == (sig # (split msg))

    verifyWorks = ML_DSA_Raw::Verify_internal expected_pk msg (take expected_sig)

    mldsaWorks = keygenWorks && signWorks && verifyWorks

